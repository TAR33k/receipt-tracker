{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetrics": true
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Trace",
      "Host.Triggers": "Trace",
      "Microsoft.Azure.WebJobs.Extensions.Storage": "Trace",
      "Azure.Storage": "Information"
    }
  },
  "functionTimeout": "00:05:00"
}
